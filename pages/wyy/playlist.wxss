/* pages/wyy/playlist.wxss */
.list{
  display: flex;
  width: 100%;
  flex-direction: column;
  align-content: center;
  justify-content: center;
  padding: 5rpx;
}
.music-item{
  display: flex;
  width: 100%;
  height: 120rpx;
  padding: 5rpx;
  justify-content: space-between;
  align-items: center;
  border-bottom: 2rpx solid black;
}
.item-title{
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  margin-right: 20rpx;
}
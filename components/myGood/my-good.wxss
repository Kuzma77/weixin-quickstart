.infos {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  flex: 0 0 50%;
  padding: 5rpx;
}
.name{
  font-weight: 700;
  font-size: 50rpx;
}
.count{
  font-size: 40rpx;
  color: orange;
}
.others{
  font-size: 35rpx;
  color: gray;
}

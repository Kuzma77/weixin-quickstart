/* components/myGood/myGood.wxss */
my-container{
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  width: 100%;
}
.header{
  color: gray;
}
image{
  width: 200rpx;
  height: 200rpx;
}
.my-img{
 flex: 0 0 30%;
}
.list {
  display: flex;
  flex-direction: column;
  height: 100%;
  width: 670rpx;
  padding: 10rpx;
  background-color: gainsboro;
}

.list-item{  
  margin: 10rpx;
  height: 200rpx;
  width: 620rpx;
  padding: 10rpx;
  border-radius: 5%;
  background-color: ghostwhite;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  flex-wrap: nowrap;
}
